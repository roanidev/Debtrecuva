

<%- include('../partials/head') %>
    <div class="wrapper">
        <%- include('../partials/sidebar') %>

            <%- include('../partials/navbar') %>

            <div class="content-page">
                <div class="container-fluid  rounded-4 py-3 px-3">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-header d-flex justify-content-between">
                                    <div class="header-title">
                                        <h4 class="card-title"><%= updatedebtlist.companyname %></h4>
                                       
                                    </div>
                                </div>
                                <div class="card-body">
                                    <form method="post" data-toggle="validator" enctype="multipart/form-data">
                                    <div class="mb-4">
                                        <h4 class="text-black">To Update Debtors List, Click on the file below to add csv file</h4>
                                      </div>
                                     
                                      
                                      <div class="col-md-12 mb-4">
                                        
                                      <div class="form-group py-4  ">
                                          <label>Attach CSV</label> 
                                          <input type="file" class="form-control image-file" name="file">
                                      </div>

                                      <div class="form-group py-4  ">
                                        <!-- <label>Company</label>  -->
                                        <input type="text" class="form-control image-file" name="company" hidden value="<%= updatedebtlist._id %>">
                                    </div>
                                    </div>
                                

                                      <div class="mt-4">                                              
                                          <button type="submit" class="btn btn-purple mb-0">upload file</button>
                                      </div>
                                      <form>
                                    
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- Page end  -->
                </div>
            </div>
    </div>

    <%- include('../partials/footer') %>